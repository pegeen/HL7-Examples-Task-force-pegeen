<!-- Multiple occurances of informationRecipient are allowed in C-CDA. --/>
<!--This first one represents the patient (who will receive it in the patient portal--/>
<informationRecipient typeCode='PRCP'>
  <!-- Only one intended Recipient allowed in C-CDA-->
  <intendedRecipient classCode ='ASSIGNED'>
    <!-- Multiple addresses and phone numbers can appear here-->
    <! -- Only one nested informationRecipient allowed in C-CDA-->
    <informationRecipient>
      <!--The name is required by C-CDA-->
      <name>
        <given>Nancy</given>
        <family>Nightingale</family>
        <suffix qualifier="AC">RN</suffix>
      </name>
    </informationRecipient>
    <!--person is scoped by the organization class-->
    <receivedOrganization>
      <name>Community Health and Hospitals</name>
      <telecom value="tel:+1(555)-555-1002" use="WP" />
      <addr use="WP">
        <streetAddressLine>Cardiac Stepdown Unit, 4B </streetAddressLine>
        <streetAddressLine>1002 Healthcare Drive </streetAddressLine> 
        <city>Ann Arbor</city>            
        <state>MI</state> 
        <postalCode>97857</postalCode>         
        <country>US</country>   
      </addr>
    </receivedOrganization>  
  </intendedRecipient>
</informationRecipient>
<!--This second one represents the portal "health care chart"--/>
<informationRecipient typeCode='PRCP'>  <!-- Only one intended Recipient allowed in C-CDA-->  <intendedRecipient classCode ='ASSIGNED'>    <!-- Multiple addresses and phone numbers can appear here-->    <! -- Only one nested informationRecipient allowed in C-CDA-->    <informationRecipient>      <!--The name is required by C-CDA-->      <name>        <given>Nancy</given>        <family>Nightingale</family>        <suffix qualifier="AC">RN</suffix>      </name>    </informationRecipient>    <!--person is scoped by the organization class-->    <receivedOrganization>      <name>Community Health and Hospitals</name>      <telecom value="tel:+1(555)-555-1002" use="WP" />      <addr use="WP">        <streetAddressLine>Cardiac Stepdown Unit, 4B </streetAddressLine>        <streetAddressLine>1002 Healthcare Drive </streetAddressLine>         <city>Ann Arbor</city>                    <state>MI</state>         <postalCode>97857</postalCode>                 <country>US</country>         </addr>    </receivedOrganization>    </intendedRecipient>
<!--This third one represents an HIE.  This document is used for multiple purposes.--/>
<informationRecipient typeCode='PRCP'>  <!-- Only one intended Recipient allowed in C-CDA-->  <intendedRecipient classCode ='ASSIGNED'>    <!-- Multiple addresses and phone numbers can appear here-->    <! -- Only one nested informationRecipient allowed in C-CDA-->    <informationRecipient>      <!--The name is required by C-CDA-->      <name>        <given>Nancy</given>        <family>Nightingale</family>        <suffix qualifier="AC">RN</suffix>      </name>    </informationRecipient>    <!--person is scoped by the organization class-->    <receivedOrganization>      <name>Community Health and Hospitals</name>      <telecom value="tel:+1(555)-555-1002" use="WP" />      <addr use="WP">        <streetAddressLine>Cardiac Stepdown Unit, 4B </streetAddressLine>        <streetAddressLine>1002 Healthcare Drive </streetAddressLine>         <city>Ann Arbor</city>                    <state>MI</state>         <postalCode>97857</postalCode>                 <country>US</country>         </addr>    </receivedOrganization>    </intendedRecipient>
<!--This fourth one represents a second HIE.  This document is used for multiple purposes.--/>
<informationRecipient typeCode='PRCP'>  <!-- Only one intended Recipient allowed in C-CDA-->  <intendedRecipient classCode ='ASSIGNED'>    <!-- Multiple addresses and phone numbers can appear here-->    <! -- Only one nested informationRecipient allowed in C-CDA-->    <informationRecipient>      <!--The name is required by C-CDA-->      <name>        <given>Nancy</given>        <family>Nightingale</family>        <suffix qualifier="AC">RN</suffix>      </name>    </informationRecipient>    <!--person is scoped by the organization class-->    <receivedOrganization>      <name>Community Health and Hospitals</name>      <telecom value="tel:+1(555)-555-1002" use="WP" />      <addr use="WP">        <streetAddressLine>Cardiac Stepdown Unit, 4B </streetAddressLine>        <streetAddressLine>1002 Healthcare Drive </streetAddressLine>         <city>Ann Arbor</city>                    <state>MI</state>         <postalCode>97857</postalCode>                 <country>US</country>         </addr>    </receivedOrganization>    </intendedRecipient>
